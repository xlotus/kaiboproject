<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FocusBorderView">
        <!--默认聚焦id-->
        <attr name="specified_focus_id" format="reference" />
        <!--borderview样式-->
        <attr name="borderview_drawable_res" format="reference" />
        <!--borderview高度-->
        <attr name="borderview_height" format="dimension" />
        <!--borderview宽度-->
        <attr name="borderview_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MenuItemView">
        <attr name="menu_icon" format="reference"/>
        <attr name="icon_size" format="dimension"/>
        <attr name="menu_name" format="reference|string"/>
        <attr name="name_color" format="color"/>
        <attr name="font_size" format="dimension"/>
        <attr name="icon_padding" format="dimension"/>
    </declare-styleable>
</resources>