<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:parentTag="android.widget.RelativeLayout"
    tools:background="@color/common_list_bg">

    <ImageView
        android:id="@+id/iv_guide_left"
        android:layout_width="@dimen/dp_50"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:src="@drawable/guide_view_left"/>

    <ImageView
        android:id="@+id/iv_guide_top"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/guide_view_top"/>

    <ImageView
        android:id="@+id/iv_guide_right"
        android:layout_width="@dimen/dp_50"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:src="@drawable/guide_view_right"/>

    <ImageView
        android:id="@+id/iv_guide_bottoom"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_68"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/guide_view_bottom"/>
</merge>
